@model PracStudentApplication.Models.Student

@{
    ViewBag.Title = "View";
}

<h2>Create Student</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <table class="table table-bordered table-striped">
        <thead>
            <tr>

                <th>@Html.DisplayNameFor(model => model.StuName)</th>
                <th>@Html.DisplayNameFor(model => model.StuEmail)</th>
                <th>@Html.DisplayNameFor(model => model.StuMarks)</th>

            </tr>
        </thead>

        <tbody>

            <tr>

                <td> @Html.EditorFor(model => model.StuName)</td>
                <td> @Html.EditorFor(model => model.StuEmail)</td>
                <td> @Html.EditorFor(model => model.StuMarks)</td>


            </tr>


        </tbody>



    </table>

    <div class="form-actions no-color">
        <input type="submit" value="Create" class="btn btn-default" /> |
        @Html.ActionLink("Back to List", "Index")
    </div>
}

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
